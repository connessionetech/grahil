{
	"id": "reaction-rule-09",
	"description": "Rule for log recording",
	"listen-to": "/logging/chunked/red5pro-server-us-7-2-recording/log/red5.log",
	"enabled": true,
	"trigger":{
		"on-payload-object": {
			"key":"data",
			"on-content": "*",
			"on-condition": "equals"
		},		
		"evaluator": "SimpleCheckEvaluator"
	},
	"response":{
		"nonce" : false,
		"action": "start_log_record",
		"reaction-func": "standard_reactions.write_log",
		"reaction-params": {
			"filepath": "/home/rajdeeprath/red5-reaction-rule-09.log"
		}
	}	
}